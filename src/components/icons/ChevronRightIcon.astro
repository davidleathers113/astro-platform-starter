---
interface Props {
    class?: string;
    width?: string;
    height?: string;
}

const { class: className = "", width = "24", height = "24" } = Astro.props;
---

<svg xmlns="http://www.w3.org/2000/svg" class={className} width={width} height={height} viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
    <polyline points="9 18 15 12 9 6"></polyline>
</svg>